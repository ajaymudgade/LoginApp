<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.LoginApp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_email_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_email_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChatApp">
        <activity
            android:name="com.example.LoginApp.RegisterActivity"
            android:exported="true"
            android:theme="@style/Theme.Noaction"/>
        <activity
            android:name="com.example.LoginApp.LoggedIn"
            android:exported="true" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="com.example.LoginApp.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Noaction">

        </activity>
    </application>

</manifest>